<?xml version="1.0" ?>
<dataset>
    <table name="member">
        <column>idmember</column>
        <column>login</column>
        <column>isadmin</column>
        <column>password</column>
    </table>
    <table name="message">
        <column>idmessage</column>
        <column>sender_idmember</column>
        <column>recipient_idmember</column>
        <column>title</column>
        <column>date</column>
        <column>content</column>
    </table>
    <table name="post">
        <column>idpost</column>
        <column>fk_idmember</column>
        <column>title</column>
        <column>date</column>
        <column>content</column>
    </table>
    <table name="question">
        <column>parent_idpost</column>
    </table>
    <table name="answer">
        <column>parent_idpost</column>
        <column>fk_idpost</column>
    </table>
</dataset>